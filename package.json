{
  "name": "nodejs-express-angular2-boilerplate",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "start": "tsc && concurrently \"npm run tsc:w\" \"npm run angular2\" \"node ./bin/www\"",
    "postinstall": "typings install",
    "gulp": "npm run angular2 && gulp",
